case ${OSTYPE} in
  cygwin*)
  export PATH="/home/riked/.rbenv/shims:${PATH}"
  export RBENV_SHELL=zsh
  source '/home/riked/.rbenv/libexec/../completions/rbenv.zsh'
  ;;
  linux*)
  export PATH="/home/user/.rbenv/shims:${PATH}"
  export RBENV_SHELL=zsh
  source '/home/user/.rbenv/libexec/../completions/rbenv.zsh'
  ;;
esac
# command rbenv rehash 2>/dev/null
rbenv() {
  local command
  command="$1"
  if [ "$#" -gt 0 ]; then
    shift
  fi

  case "$command" in
  rehash|shell)
    eval "$(rbenv "sh-$command" "$@")";;
  *)
    command rbenv "$command" "$@";;
  esac
}
