{
  "editor.renderWhitespace": "all",
  "editor.fontFamily": "Hack Nerd Font",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "window.zoomLevel": 0,
  "python.formatting.provider": "autopep8",
  "python.venvPath": ".venv",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--max-line-length", "120"],
  "python.linting.enabled": true,
  "python.sortImports.args": ["-rc", "--atomic"],
  "dart.lineLength": 100,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.leader": "<space>",
  "workbench.iconTheme": "material-icon-theme",
  "autoDocstring.docstringFormat": "google",
  "explorer.confirmDelete": false,
  "editor.suggestSelection": "first",
  "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: Google, ColumnLimit: 0}",
  "[c]": {
    "editor.wordBasedSuggestions": false
  },
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.cStandard": "c11",
  "C_Cpp.default.intelliSenseMode": "clang-x86",
  "[javascript]": {
    "editor.tabSize": 4,
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.formatOnType": false
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.formatOnType": false
  },
  "typescript.preferences.quoteStyle": "single",
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": false,
    "editor.letterSpacing": -1,
    "files.trimTrailingWhitespace": false,
    "editor.defaultFormatter": "unifiedjs.vscode-remark"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": true
  },
  "[terraform]": {
    "editor.tabSize": 2
  },
  "go.useLanguageServer": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.variables": {
    "lang": "ja"
  },
  "explorer.confirmDragAndDrop": false,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "editor.codeActionsOnSave": ["source.organizeImpors", "source.fixAll.eslint"],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "hediet.vscode-drawio.local-storage": "",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "prettier.jsxSingleQuote": true,
  "workbench.colorTheme": "GitHub Dark",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "redhat.telemetry.enabled": true,
  "go.toolsManagement.autoUpdate": true,
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "githubPullRequests.pullBranch": "never"
}
